    "settings": {
      "code": "<script>(function(){var url='https://maxhair.cz/wp-content/uploads/2026/02/Layer0.png';function fix(){var h=document.querySelector('#brx-header,#main-header,header');if(!h)return;var links=h.querySelectorAll('a[href=\"/\"],a[href*=\"maxhair.cz/\"]');for(var i=0;i<links.length;i++){var a=links[i];if(a.closest('nav'))continue;var txt=(a.textContent||'').trim();if(txt.indexOf('MAX')>=0||txt.indexOf('HAIR')>=0||(!a.querySelector('img')&&txt.length<30)){var img=a.querySelector('img');if(img)img.src=url;else a.innerHTML='<img src=\"'+url+'\" alt=\"MaxHair\" style=\"height:40px;width:auto;max-height:50px\">';return}}}if(document.readyState==='loading')document.addEventListener('DOMContentLoaded',fix);else fix();setTimeout(fix,300);setTimeout(fix,1000);})();</script>\n<!-- REFERENCE HERO SECTION - stejný styl jako homepage hero -->\n<section class=\"reference-hero-section\">\n  <div class=\"mh-hero-bg\">\n    <div class=\"mh-hero-overlay\"></div>\n    <video autoplay muted loop playsinline class=\"mh-hero-video\" poster=\"https://maxhair.cz/wp-content/uploads/2026/02/Layer0.png\">\n      <source src=\"https://mediumseagreen-gaur-406121.hostingersite.com/wp-content/uploads/2025/12/WhatsApp-Video-2025-12-13-at-14.56.52_6de6a47b.mp4\" type=\"video/mp4\">\n    </video>\n  </div>\n  <div class=\"reference-hero-content\">\n    <span class=\"mh-badge\">REFERENCE</span>\n    <h1 class=\"hero-title\" style=\"color: #ffffff !important; -webkit-text-fill-color: #ffffff !important;\">Spokojení klienti mluví za nás</h1>\n  </div>\n</section>\n\n<style>\n  @import url('https://fonts.googleapis.com/css2?family=Poppins:wght@300;400;500;600;700;800&family=Inter:wght@300;400;500;600;700&display=swap');\n\n  :root {\n    --primary-gold: #E5C158;\n    --secondary-gold: #D4AF37;\n    --dark-gray: #1A1A1A;\n    --black: #000000;\n    --white: #FFFFFF;\n    --light-gray: #F5F5F5;\n    --medium-gray: #333333;\n  }\n\n  .reference-hero-section {\n    width: 100%;\n    max-width: 100%;\n    box-sizing: border-box;\n    min-height: 70vh;\n    position: relative;\n    display: flex;\n    align-items: center;\n    justify-content: center;\n    overflow: hidden;\n    padding: 120px 24px 80px;\n    text-align: center;\n  }\n\n  .reference-hero-section .mh-hero-bg {\n    position: absolute;\n    inset: 0;\n    z-index: 0;\n    background: var(--dark-gray);\n  }\n\n  .reference-hero-section .mh-hero-overlay {\n    position: absolute;\n    inset: 0;\n    background: linear-gradient(135deg, rgba(26, 26, 26, 0.75), rgba(0, 0, 0, 0.8));\n    z-index: 1;\n  }\n\n  .reference-hero-section .mh-hero-video {\n    width: 100%;\n    height: 100%;\n    object-fit: cover;\n    opacity: 0.7;\n  }\n\n  .reference-hero-content {\n    position: relative;\n    z-index: 2;\n    max-width: 1000px;\n    width: 100%;\n  }\n\n  @media (max-width: 768px) {\n    .reference-hero-section .mh-hero-video { display: none; }\n    .reference-hero-section .mh-hero-bg { background: linear-gradient(135deg, rgba(26, 26, 26, 0.85), rgba(0, 0, 0, 0.9)); }\n  }\n\n  .reference-hero-section .section-badge,\n  .reference-hero-section .mh-badge {\n    display: inline-block;\n    padding: 8px 24px;\n    background: var(--primary-gold) !important;\n    color: #000000 !important;\n    -webkit-text-fill-color: #000000 !important;\n    font-family: 'Inter', sans-serif;\n    font-size: 13px;\n    font-weight: 600;\n    letter-spacing: 2px;\n    text-transform: uppercase;\n    border-radius: 30px;\n    margin-bottom: 24px;\n    border: none;\n  }\n\n  .reference-hero-section .hero-title,\n  .reference-hero-section .reference-hero-content .hero-title,\n  .reference-hero-section h1,\n  .reference-hero-section h1.hero-title {\n    font-family: 'Poppins', sans-serif;\n    font-size: clamp(36px, 5vw, 64px);\n    font-weight: 700;\n    color: #ffffff !important;\n    -webkit-text-fill-color: #ffffff !important;\n    margin: 0 0 24px 0;\n    letter-spacing: normal;\n    line-height: 1.2;\n  }\n\n  .reference-hero-section .hero-description,\n  .reference-hero-section .reference-hero-content .hero-description,\n  .reference-hero-section p,\n  .reference-hero-section p.hero-description {\n    font-family: 'Inter', sans-serif;\n    font-size: clamp(16px, 2vw, 20px);\n    color: #1A1A1A !important;\n    -webkit-text-fill-color: #1A1A1A !important;\n    opacity: 0.85;\n    margin: 0;\n    line-height: 1.6;\n  }\n\n  @media (max-width: 768px) {\n    .reference-hero-section {\n      padding: 100px 24px 60px 24px;\n      min-height: 100vh; min-height: 100svh;\n    }\n  }\n\n  /* Fix Bricks wrapper backgrounds */\n  .brxe-section {\n    background: transparent !important;\n    padding: 0 !important;\n    margin: 0 !important;\n  }\n  .brxe-container {\n    background: transparent !important;\n    padding: 0 !important;\n    margin: 0 !important;\n    max-width: 100% !important;\n    width: 100% !important;\n  }\n  body {\n    background: #FFFFFF !important;\n  }\n</style>",
