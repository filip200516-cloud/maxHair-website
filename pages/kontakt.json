-x: hidden !important;\n      padding: 60px 24px !important;\n      max-width: 100% !important;\n      box-sizing: border-box !important;\n    }\n    #brxe-kontakt_why_code > div > div:nth-child(2) {\n      grid-template-columns: 1fr !important;\n      gap: 24px !important;\n      min-width: 0 !important;\n    }\n  }\n\n  @media (max-width: 480px) {\n    #brxe-kontakt_why_code > div {\n      padding: 40px 16px !important;\n    }\n  }\n\n  #brxe-kontakt_why_section,\n  #brxe-kontakt_why_container,\n  #brxe-kontakt_why_code {\n    overflow-x: hidden !important;\n    max-width: 100% !important;\n  }\n\n  \n</style>\n<script>\n(function(){\n  function fix(){\n    var ids=['kontakt_why_section','kontakt_why_container','kontakt_why_code'];\n    ids.forEach(function(id){\n      var el=document.getElementById('brxe-'+id);\n      if(el){\n        el.style.setProperty('background','#FFFFFF','important');\n        el.style.setProperty('background-color','#FFFFFF','important');\n        var ch=el.children;\n        for(var i=0;i<ch.length;i++){\n          ch[i].style.setProperty('background','#FFFFFF','important');\n          ch[i].style.setProperty('background-color','#FFFFFF','important');\n        }\n      }\n    });\n  }\n  fix();\n  document.addEventListener('DOMContentLoaded',fix);\n  window.addEventListener('load',fix);\n  setTimeout(fix,500);\n  setTimeout(fix,1500);\n})();\n</script>",
